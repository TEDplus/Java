
public class Test_02 {

	public static void main(String[] args) {
		int count = 0, line =1;
		for(int i = 65 ; i<=90 ; i++) {
			if(line % 1 == 0) System.out.print((char)(i + 32)+"\t");//소문자
			else System.out.print((char)(i) + "\t"); //대문자 출력
			count++;
			if(count % 5 == 0 ) {
				System.out.println(); line++;
			}
			
		}
	
	}

}
