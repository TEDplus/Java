
public class Test_02 {

	public static void main(String[] args) {
		int count = 0, line = 10;
		for(int i = 65 ; i<=90 ; i++) {
			if (count % 1 == 0) { System.out.print((char)(i) + "\t"); //대문자 출력
			count++;
			//line++;
			}
			else System.out.print((char)(i) + "\t"); //대문자 출력
			
			
		}
	
	}

}
